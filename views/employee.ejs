<!DOCTYPE html>          
<html>          
<head> <!-- www.techstrikers.com -->          
<title>Manual Initialization Using Bootstrap</title>          


<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
<!-- src="reviewEmpapp.js"></script> --> 

<!-- <script src="twowaydy.js"></script> -->
<script src="myview.js"></script> 


         
</head>    
     
        
<body ng-app = "mainApp">
  


  <div ng-controller="empCtrl">

    
    <table border="1" style="width:50%">
     <tr>
      <td>ID: </td> 
    <td>Name</td>
    <td>Age </td>
     <td>status </td>
      <td>title</td> 
       <td>salary </td> 
    <td>Image</td>
  </tr>
      <tr ng-repeat="x in employees ">
     <td>{{ x._id }}</td>
    <td>{{ x.name }}</td>
    <td>{{ x.age }}</td>
      <td>{{ x.status }}</td>
       <td>{{ x.title }}</td>
        <td>{{ x.salary }}</td>
         <td><img width="100" ng-src="{{x.image}}"></td>
  </tr>
   </table>
  </div>
   <br>
 
  <div ng-controller = "CalcController">
        <p>pppoo{{number}}</p>
         <p>Enter a number: <input type = "number" ng-model = "number" /></p>
         <button ng-click = "square()">X<sup>2</sup></button>
         <p>Result: {{result}}</p>
  </div>
  
 


 <script>

  var mainapp = angular.module("mainApp", ['viewApp']);
   var url='https://mywebdemost.mybluemix.net/employees';
   

   mainapp.controller("empCtrl", function($scope, $http) {
     
     $scope.empls={};
    $http.get(url).success( function(response) {
      $scope.employees = response.data;
    });
    
   
    
});
  
 </script> 

        
  
</body>          
</html>   
